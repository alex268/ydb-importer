<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.0.0:exec</goal>
            </goals>
            <properties>
                <exec.vmArgs></exec.vmArgs>
                <exec.args>${exec.vmArgs} -classpath %classpath ${exec.mainClass} ${exec.appArgs}</exec.args>
                <exec.appArgs>scripts/sample-postgres-real.xml</exec.appArgs>
                <exec.mainClass>ydb.importer.YdbImporter</exec.mainClass>
                <exec.executable>java</exec.executable>
                <Env.YDB_SERVICE_ACCOUNT_KEY_FILE_CREDENTIALS>/home/zinal/key-ydb-sa1.json</Env.YDB_SERVICE_ACCOUNT_KEY_FILE_CREDENTIALS>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.0.0:exec</goal>
            </goals>
            <properties>
                <exec.vmArgs>-agentlib:jdwp=transport=dt_socket,server=n,address=${jpda.address}</exec.vmArgs>
                <exec.args>${exec.vmArgs} -classpath %classpath ${exec.mainClass} ${exec.appArgs}</exec.args>
                <exec.appArgs>scripts/sample-postgres-real.xml</exec.appArgs>
                <exec.mainClass>ydb.importer.YdbImporter</exec.mainClass>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
                <Env.YDB_SERVICE_ACCOUNT_KEY_FILE_CREDENTIALS>/home/zinal/key-ydb-sa1.json</Env.YDB_SERVICE_ACCOUNT_KEY_FILE_CREDENTIALS>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.0.0:exec</goal>
            </goals>
            <properties>
                <exec.vmArgs></exec.vmArgs>
                <exec.args>${exec.vmArgs} -classpath %classpath ${exec.mainClass} ${exec.appArgs}</exec.args>
                <exec.mainClass>ydb.importer.YdbImporter</exec.mainClass>
                <exec.executable>java</exec.executable>
                <exec.appArgs>scripts/sample-postgres-real.xml</exec.appArgs>
            </properties>
        </action>
    </actions>
